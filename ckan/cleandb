psql -h 172.17.0.1 -p 5432 -U postgres -f sql/clean_00_cleandb.sql
docker run -p 5000:5000 -p 80:80 --link db:db --link solr:solr ckan/ckan paster db init -c /etc/ckan/default/development.ini
